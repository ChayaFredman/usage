<h1>Listing rx_by_days</h1>

<table>
  <tr>
    <th>rx</th>
    <th>date1</th>
    <th>count_minutes</th>
  </tr>

<% @rx_by_days.each do |rx_by_day| %>
  <tr>
    <td><%=rx_by_day.rx%></td>
    <td><%=rx_by_day.date1%></td>
    <td><%=rx_by_day.count_minutes%></td>
    <td><%= link_to 'Show', rx_by_day %></td>
    <td><%= link_to 'Edit', edit_rx_by_day_path(rx_by_day) %></td>
    <td><%= link_to 'Destroy', rx_by_day, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Rx by day', new_rx_by_day_path %>

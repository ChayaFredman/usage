<h1>Listing rx_by_hours</h1><br>
<%= link_to 'rx by days', rx_by_days_path %>
<%= link_to 'tx by days', tx_by_days_path %>
<%= link_to 'rx by hours', rx_by_hours_path %>
<%= link_to 'tx by hours', tx_by_hours_path %>
<br />
<br />
<br />
<input type="text" id="txsearch" value="search"></input> 
<br />

<table id="rs">
  <tr>
     <th>rx</th>
    <th>date</th>
    <th>hour</th>
    <th>usage minutes </th>
  </tr>

<% @rx_by_hours.each do |rx_by_hour| %>
  <tr>
     <td><%=rx_by_hour.rx%></td>
    <td><%=rx_by_hour.date1%></td>
    <td><%=rx_by_hour.hour%></td>
    <td><%= "%.2f" % (((rx_by_hour.count_seconds).to_f)/60)%> </td>
   
  </tr>
<% end %>
</table>

<br />



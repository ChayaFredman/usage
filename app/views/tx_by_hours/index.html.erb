<h1>Listing tx_by_hours</h1><br>
<%= link_to 'rx by days', rx_by_days_path %>
<%= link_to 'tx by days', tx_by_days_path %>
<%= link_to 'rx by hours', rx_by_hours_path %>
<%= link_to 'tx by hours', tx_by_hours_path %>
<br>
<br>
<br>
<input type="text" id="txsearch" value="search"></input> 
<br />

<table>
  <tr>
    <th>tx</th>
    <th>date</th>
    <th>hour</th>
    <th>usage minutes</th>
  </tr>

<% @tx_by_hours.each do |tx_by_hour| %>
  <tr>
    <td><%=tx_by_hour.tx%></td>
    <td><%=tx_by_hour.date1%></td>
    <td><%=tx_by_hour.hour%></td>
    <td><%= "%.2f" % (((tx_by_hour.count_seconds).to_f)/60)%></td>
    
  </tr>
<% end %>
</table>

<br />

